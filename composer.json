{
    "name": "hexes/laravel-vue-route-generator",
    "description": "A package to generate and manage routes for Vue applications from Laravel routes.",
    "type": "library",
    "autoload": {
      "psr-4": {
        "App\\": "src/Laravel/"
      }
    },
    "require": {
      "php": "^7.3|^8.0",
      "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "App\\Providers\\RouteServiceProvider"
        ]
      }
    }
  }
  